{
    "generators": {
        "c": {
            "signature": {
                "name": "C",
                "id": "template-c",
                "aliases": ["c", "clang"]
            },
            "pkgmanager": {
                "$ref": "#/pkgmanager/node"
            },
            "paths": [
                {
                    "$ref": "#/files/paths/github"
                },
                {
                    "$ref": "#/files/paths/husky"
                },
                {
                    "$ref": "#/files/paths/vscode"
                },
                {
                    "$ref": "#/files/paths/src"
                },
                {
                    "$ref": "#/files/paths/changelogrc"
                },
                {
                    "$ref": "#/files/paths/commitlintrc"
                },
                {
                    "$ref": "#/files/paths/czrc"
                },
                {
                    "$ref": "#/files/paths/editorconfig"
                },
                {
                    "$ref": "#/files/paths/gitattributes"
                },
                {
                    "$ref": "#/files/paths/gitignore"
                },
                {
                    "$ref": "#/files/paths/gitversion"
                },
                {
                    "$ref": "#/files/paths/lintstagedrc"
                },
                {
                    "$ref": "#/files/paths/makefile"
                },
                {
                    "$ref": "#/files/paths/nvmrc"
                },
                {
                    "$ref": "#/files/paths/packagejson"
                },
                {
                    "$ref": "#/files/paths/prettierrc"
                },
                {
                    "$ref": "#/files/paths/prettierignore"
                },
                {
                    "$ref": "#/files/paths/license"
                },
                {
                    "$ref": "#/files/paths/readme"
                }
            ]
        },
        "ts": {
            "signature": {
                "name": "Typescript",
                "id": "template-ts",
                "aliases": ["ts", "typescript"]
            },
            "pkgmanager": {
                "$ref": "#/pkgmanager/node"
            }
        },
        "js": {
            "signature": {
                "name": "Javascript",
                "id": "template-js",
                "aliases": ["js", "javascript"]
            },
            "pkgmanager": {
                "$ref": "#/pkgmanager/node"
            }
        },
        "py": {
            "signature": {
                "name": "Python",
                "id": "template-py",
                "aliases": ["py", "python"]
            }
        },
        "html": {
            "signature": {
                "name": "HTML",
                "id": "template-html",
                "aliases": ["html", "web", "vanilla"]
            },
            "pkgmanager": {
                "$ref": "#/pkgmanager/node"
            }
        },
        "simpl": {
            "signature": {
                "name": "Simpl",
                "id": "template-simpl",
                "aliases": ["simpl"]
            },
            "pkgmanager": {
                "$ref": "#/pkgmanager/node"
            }
        }
    },
    "files": {
        "directory": "templates",
        "paths": {
            "readme": {
                "from": "README.md",
                "to": "README.md"
            },
            "license": {
                "from": "LICENSE",
                "to": "LICENSE"
            },
            "prettierignore": {
                "from": "prettierignore",
                "to": ".prettierignore"
            },
            "prettierrc": {
                "from": "prettierrc.json",
                "to": ".prettierrc.json"
            },
            "packagejson": {
                "from": "package.json.ejs",
                "to": "package.json"
            },
            "tsconfig": {
                "from": "tsconfig.json",
                "to": "tsconfig.json"
            },
            "tsupconfig": {
                "from": "tsup.config.json",
                "to": "tsup.config.json"
            },
            "tsupschema": {
                "from": "tsup.schema.json",
                "to": "tsup.schema.json"
            },
            "nvmrc": {
                "from": "nvmrc",
                "to": ".nvmrc"
            },
            "npmrc": {
                "from": "npmrc",
                "to": ".npmrc"
            },
            "env": {
                "from": "env",
                "to": ".env"
            },
            "makefile": {
                "from": "Makefile",
                "to": "Makefile"
            },
            "lintstagedrc": {
                "from": "lintstagedrc.json",
                "to": ".lintstagedrc.json"
            },
            "gitversion": {
                "from": "GitVersion.yml",
                "to": "GitVersion.yml"
            },
            "gitignore": {
                "from": "gitignore",
                "to": ".gitignore"
            },
            "gitattributes": {
                "from": "gitattributes",
                "to": ".gitattributes"
            },
            "editorconfig": {
                "from": "editorconfig",
                "to": ".editorconfig"
            },
            "czrc": {
                "from": "czrc",
                "to": ".czrc"
            },
            "commitlintrc": {
                "from": "commitlintrc.json",
                "to": ".commitlintrc.json"
            },
            "changelogrc": {
                "from": "changelogrc.json",
                "to": ".changelogrc.json"
            },
            "changelog": {
                "from": "CHANGELOG.md",
                "to": "CHANGELOG.md"
            },
            "github": {
                "from": "github",
                "to": ".github"
            },
            "husky": {
                "from": "husky",
                "to": ".husky"
            },
            "vscode": {
                "from": "vscode",
                "to": ".vscode"
            },
            "src": {
                "from": "src",
                "to": "src"
            },
            "public": {
                "from": "public",
                "to": "public"
            },
            "assets": {
                "from": "assets",
                "to": "assets"
            },
            "ui": {
                "from": "ui",
                "to": "ui"
            }
        }
    },
    "pkgmanager": {
        "node": {
            "default": "pnpm",
            "choices": ["pnpm", "yarn", "npm"]
        }
    }
}
