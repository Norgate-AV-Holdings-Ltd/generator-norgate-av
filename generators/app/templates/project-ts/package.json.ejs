{
    "name": <%- JSON.stringify(name) %>,
    "displayName": <%- JSON.stringify(displayName) %>,
    "description": <%- JSON.stringify(description) %>,
    "version": "0.0.0-development",
    "license": "MIT",
    "main": "./dist/index.js",
    "repository": {
        "type": "git",
        "url": ""
    },
    "bugs": {
        "url": ""
    },
    "engines": {
        "node": ">=<% node.split(".")[0] %>"
    },
    "scripts": {
        "commit": "git-cz",
        "clean": "rimraf dist",
        "lint": "tsc --noEmit",
        "pretty:fix": "prettier --write .",
        "prebuild": "<%- pkg %> clean",
        "build": "tsup",
        "prestart": "<%- pkg %> lint && <%- pkg %> build",
        "start": "cross-env NODE_ENV=production node dist/index.js",
        "dev": "nodemon --exec ts-node src/index.ts",
        "prepare": "husky install"
    },
    "devDependencies": {
        <%- devDep("@commitlint/cli") %>,
        <%- devDep("@commitlint/config-conventional") %>,
        <%- devDep("@types/config") %>,
        <%- devDep("@types/node") %>,
        <%- devDep("@types/nodemon") %>,
        <%- devDep("commitizen") %>,
        <%- devDep("cross-env") %>,
        <%- devDep("cz-conventional-changelog") %>,
        <%- devDep("husky") %>,
        <%- devDep("lint-staged") %>,
        <%- devDep("nodemon") %>,
        <%- devDep("prettier") %>,
        <%- devDep("rimraf") %>,
        <%- devDep("terser") %>,
        <%- devDep("ts-node") %>,
        <%- devDep("tsup") %>,
        <%- devDep("typescript") %>
    },
    "dependencies": {
        <%- dep("config") %>,
        <%- dep("dotenv") %>,
        <%- dep("envalid") %>
    }
}
