{
    "name": "generator-norgate-av",
    "version": "1.0.6",
    "description": "A collection of various project boilerplates",
    "keywords": [
        "yeoman-generator",
        "project",
        "javascript",
        "typescript",
        "boilerplate"
    ],
    "license": "MIT",
    "main": "dist/app/index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/Norgate-AV/generator-norgate-av.git"
    },
    "bugs": {
        "url": "https://github.com/Norgate-AV/generator-norgate-av/issues"
    },
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=16"
    },
    "scripts": {
        "commit": "git-cz",
        "lint": "tsc --noEmit",
        "clean": "rimraf dist",
        "prebuild": "pnpm clean",
        "build": "tsup",
        "postbuild": "pnpm copy:templates",
        "copy:templates": "ncp src/app/templates dist/app/templates && ncp src/app/dependencies dist/app/dependencies",
        "test": "vitest --no-threads",
        "pretty:fix": "prettier --write .",
        "doctoc": "doctoc --title '## Contents ðŸ“–' README.md",
        "contrib:add": "all-contributors add",
        "contrib:generate": "all-contributors generate",
        "contrib:check": "all-contributors check",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.0.2",
        "@commitlint/config-conventional": "^17.0.2",
        "@semantic-release/changelog": "^6.0.1",
        "@semantic-release/git": "^10.0.1",
        "@types/node": "^20.6.5",
        "@types/which": "^3.0.0",
        "@types/yeoman-assert": "^3.1.2",
        "@types/yeoman-generator": "^5.2.12",
        "@types/yeoman-test": "^4.0.4",
        "@types/yosay": "^2.0.1",
        "all-contributors-cli": "^6.20.0",
        "commitizen": "^4.3.0",
        "cpy-cli": "^5.0.0",
        "cz-conventional-changelog": "3.3.0",
        "doctoc": "^2.2.0",
        "husky": "^8.0.1",
        "lint-staged": "^14.0.1",
        "ncp": "^2.0.0",
        "prettier": "^3.0.3",
        "rimraf": "^5.0.2",
        "semantic-release": "^22.0.5",
        "terser": "^5.20.0",
        "tsup": "^7.2.0",
        "typescript": "^5.2.2",
        "vitest": "^0.34.5",
        "yeoman-assert": "^3.1.1",
        "yeoman-environment": "^3.9.1",
        "yeoman-test": "^7.4.0"
    },
    "dependencies": {
        "chalk": "^4.1.2",
        "which": "^4.0.0",
        "yeoman-generator": "^5.6.1",
        "yosay": "^2.0.2"
    }
}
