{
    "name": "generator-norgate-av",
    "version": "0.0.0-development",
    "description": "A collection of various project boilerplates",
    "keywords": [
        "",
        "yeoman-generator"
    ],
    "license": "MIT",
    "main": "generators/index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/Norgate-AV-Solutions-Ltd/generator-norgate-av.git"
    },
    "bugs": {
        "url": "https://github.com/Norgate-AV-Solutions-Ltd/generator-norgate-av/issues"
    },
    "files": [
        "generators"
    ],
    "engines": {
        "node": ">=14"
    },
    "scripts": {
        "pretest": "eslint .",
        "test": "jest"
    },
    "devDependencies": {
        "eslint": "^8.15.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-xo": "^0.40.0",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "^8.0.1",
        "jest": "^28.1.0",
        "lint-staged": "^12.4.1",
        "prettier": "^2.6.2",
        "yeoman-assert": "^3.1.1",
        "yeoman-test": "^6.3.0"
    },
    "dependencies": {
        "chalk": "^5.0.1",
        "yeoman-generator": "^5.6.1",
        "yosay": "^2.0.2"
    },
    "jest": {
        "testEnvironment": "node"
    },
    "lint-staged": {
        "*.js": [
            "eslint --fix",
            "git add"
        ],
        "*.json": [
            "prettier --write",
            "git add"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "eslintConfig": {
        "extends": [
            "xo",
            "prettier"
        ],
        "env": {
            "jest": true,
            "node": true
        },
        "rules": {
            "prettier/prettier": "error"
        },
        "plugins": [
            "prettier"
        ]
    }
}
