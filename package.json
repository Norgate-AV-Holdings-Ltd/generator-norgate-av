{
    "name": "generator-norgate-av",
    "version": "2.4.0",
    "description": "A collection of various project boilerplates",
    "keywords": [
        "yeoman-generator",
        "project",
        "javascript",
        "typescript",
        "boilerplate",
        "python",
        "node-cli",
        "crestron-simpl"
    ],
    "license": "MIT",
    "main": "dist/generators/app/index.js",
    "typings": "dist/generators/app/index.d.ts",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/Norgate-AV/generator-norgate-av.git"
    },
    "bugs": {
        "url": "https://github.com/Norgate-AV/generator-norgate-av/issues"
    },
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=18"
    },
    "scripts": {
        "commit": "git-cz",
        "lint": "tsc && eslint . --fix",
        "clean": "rimraf dist",
        "prebuild": "pnpm clean && pnpm lint",
        "build": "tsup",
        "copy:deps": "ncp src/dependencies dist/generators/app/dependencies",
        "copy:templates": "ncp templates dist/generators/app/templates",
        "copy:usage": "ncp src/USAGE dist/generators/app/USAGE",
        "copy:files": "pnpm copy:deps && pnpm copy:templates && pnpm copy:usage",
        "test": "vitest --no-threads",
        "pretty:fix": "prettier --write .",
        "doctoc": "doctoc --title '## Contents ðŸ“–' README.md",
        "contrib:add": "all-contributors add",
        "contrib:generate": "all-contributors generate",
        "contrib:check": "all-contributors check",
        "prepare": "husky"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.4.0",
        "@commitlint/config-conventional": "^19.2.2",
        "@eslint/js": "^9.8.0",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "@types/eslint__js": "^8.42.3",
        "@types/node": "^22.1.0",
        "@types/which": "^3.0.4",
        "@types/yeoman-assert": "^3.1.4",
        "@types/yosay": "^2.0.3",
        "@yeoman/types": "^1.2.0",
        "all-contributors-cli": "^6.26.1",
        "commitizen": "^4.3.0",
        "cz-conventional-changelog": "3.3.0",
        "doctoc": "^2.2.1",
        "eslint": "^9.8.0",
        "eslint-config-prettier": "^9.1.0",
        "husky": "^9.1.4",
        "lint-staged": "^15.2.8",
        "ncp": "^2.0.0",
        "prettier": "^3.3.3",
        "rimraf": "^6.0.1",
        "semantic-release": "^24.0.0",
        "terser": "^5.31.5",
        "tsup": "^8.2.4",
        "type-fest": "^4.24.0",
        "typescript": "^5.5.4",
        "typescript-eslint": "^8.0.1",
        "vite-node": "^2.0.5",
        "vitest": "^0.34.6",
        "yeoman-assert": "^3.1.1",
        "yeoman-environment": "^4.4.1",
        "yeoman-test": "^8.3.0"
    },
    "dependencies": {
        "axios": "^1.7.3",
        "chalk": "^5.3.0",
        "find-up": "^7.0.0",
        "json-refs": "^3.0.15",
        "mem-fs": "^4.1.0",
        "which": "^4.0.0",
        "yeoman-generator": "^7.3.2",
        "yosay": "^3.0.0"
    }
}
