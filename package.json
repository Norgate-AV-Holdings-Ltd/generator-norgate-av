{
    "name": "generator-norgate-av",
    "version": "2.0.0",
    "description": "A collection of various project boilerplates",
    "keywords": [
        "yeoman-generator",
        "project",
        "javascript",
        "typescript",
        "boilerplate"
    ],
    "license": "MIT",
    "main": "dist/app/app.js",
    "typings": "dist/app/app.d.ts",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/Norgate-AV/generator-norgate-av.git"
    },
    "bugs": {
        "url": "https://github.com/Norgate-AV/generator-norgate-av/issues"
    },
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=16"
    },
    "scripts": {
        "commit": "git-cz",
        "lint": "tsc && eslint . --fix",
        "clean": "rimraf dist",
        "prebuild": "pnpm clean",
        "build": "tsup",
        "postbuild": "",
        "copy:templates": "ncp src/template-files dist/app/template-files && ncp src/dependencies dist/app/dependencies",
        "test": "vitest --no-threads",
        "pretty:fix": "prettier --write .",
        "doctoc": "doctoc --title '## Contents ðŸ“–' README.md",
        "contrib:add": "all-contributors add",
        "contrib:generate": "all-contributors generate",
        "contrib:check": "all-contributors check",
        "prepare": "husky",
        "preyo": "pnpm build",
        "yo": "yo ./dist/app/app.js"
    },
    "devDependencies": {
        "@commitlint/cli": "^18.6.0",
        "@commitlint/config-conventional": "^18.6.0",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "@types/config": "^3.3.3",
        "@types/node": "^20.11.16",
        "@types/which": "^3.0.3",
        "@types/yeoman-assert": "^3.1.4",
        "@types/yeoman-generator": "^5.2.14",
        "@types/yeoman-test": "^4.0.6",
        "@types/yosay": "^2.0.3",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "@yeoman/types": "^1.1.2",
        "all-contributors-cli": "^6.26.1",
        "commitizen": "^4.3.0",
        "cz-conventional-changelog": "3.3.0",
        "doctoc": "^2.2.1",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "husky": "^9.0.10",
        "lint-staged": "^15.2.2",
        "ncp": "^2.0.0",
        "prettier": "^3.2.5",
        "rimraf": "^5.0.5",
        "semantic-release": "^23.0.1",
        "terser": "^5.27.0",
        "tsup": "^8.0.1",
        "type-fest": "^4.10.2",
        "typescript": "^5.3.3",
        "vite-node": "^1.2.2",
        "vitest": "^1.2.2",
        "yeoman-assert": "^3.1.1",
        "yeoman-environment": "^4.3.0",
        "yeoman-test": "^8.2.0"
    },
    "dependencies": {
        "axios": "^1.6.7",
        "chalk": "^5.3.0",
        "config": "^3.3.11",
        "find-up": "^7.0.0",
        "json-refs": "^3.0.15",
        "mem-fs": "^4.0.0",
        "which": "^4.0.0",
        "yeoman-generator": "^7.1.1",
        "yosay": "^3.0.0"
    }
}
