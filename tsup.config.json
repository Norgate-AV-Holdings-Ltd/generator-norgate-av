{
    "entry": {
        "index": "src/app.ts"
    },
    "format": ["esm"],
    "target": "node20",
    "outDir": "dist/generators/app",
    "minify": "terser",
    "dts": true,
    "minifyWhitespace": true,
    "minifyIdentifiers": true,
    "minifySyntax": true,
    "treeshake": true,
    "onSuccess": "pnpm copy:files"
}
